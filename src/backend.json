{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "patternProperties": {
      "^[a-zA-Z0-9]+$": {
        "type": "object",
        "properties": {
          "age": {"type": "number"},
          "bio": {"type": "string"},
          "image": {"type": "string", "format": "uri"},
          "name": {"type": "string"},
          "pronouns": {"type": "string"}
        },
        "required": ["age", "bio", "image", "name"],
        "additionalProperties": false
      }
    }
  }